%Adjunctions
\newcommand{\backforth}[4]{{#1}\colon{#2} \rightleftarrows {#3}\noloc{#4}}
\newcommand{\adjto}[4]{
	\begin{tikzcd}[ampersand replacement = \&]
		{#1:#2} \& {#3:#4}
		\arrow[""{name=0, anchor=center, inner sep=0}, shift left=2, from=1-1, to=1-2]
		\arrow[""{name=1, anchor=center, inner sep=0}, shift left=2, from=1-2, to=1-1]
		\arrow["\dashv"{anchor=center, rotate=-90}, draw=none, from=0, to=1]
	\end{tikzcd}}

% Bijection
\newcommand{\bijection}{\similarrightarrow}
\newcommand{\bijto}[2]{{#1}\bijection{#2}}

% Cofibration
