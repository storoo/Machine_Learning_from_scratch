\newcommand{\injectionop}{\leftarrowtail}
\newcommand{\into}[2]{{#1} \injection {#2}}
\newcommand{\inot}[2]{{#1} \injectionop {#2}}

% Mapsto
\newcommand{\mapstoarrow}{\mapsto}
\newcommand{\goesto}[2]{{#1} \mapstoarrow {#2}}

% Parallel arrows
